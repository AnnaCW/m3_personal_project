<div class="row">

  <div class="col-md-6">
      <h2><%= @presenter.find(params[:id])["name"] %> Playlists</h2>
      <table id="browse-categories">
        <% @presenter.category_playlists(params[:id]).each do |playlist| %>
            <tr>
              <td><%= playlist.name %></td>
            </tr>
        <% end %>
      </table>

  </div>
